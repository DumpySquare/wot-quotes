{
  "name": "wot-quotes",
  "version": "0.0.1",
  "description": "Serverless function that returns random WOT quotes.",
  "author": "Brian Andrews <sbardian@gmail.com>",
  "main": "functions/__main__.js",
  "private": true,
  "stdlib": {
    "build": "faaslang",
    "name": "sbardian/wotQuotes",
    "timeout": 10000,
    "personalize": {
      "keys": [],
      "user": []
    }
  },
  "scripts": {
    "cm": "git-cz",
    "lint": "eslint .",
    "test": "jest ."
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "graphql-request": "1.8.2",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1",
    "semantic-release": "16.0.4"
  }
}
